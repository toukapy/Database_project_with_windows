<AnchorPane prefHeight="555.0" prefWidth="814.0" style="-fx-background-color: lavender;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.GuideAllTripYearController">
   <children>
      <Label layoutX="66.0" layoutY="66.0" text="Type the date">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="dateField" layoutX="198.0" layoutY="62.0" prefHeight="35.0" prefWidth="385.0" />
      <Button fx:id="enterBtn" layoutX="615.0" layoutY="62.0" mnemonicParsing="false" onAction="#onClickEnter" prefHeight="35.0" prefWidth="133.0" text="Enter" />
      <TableView fx:id="tblTrip" layoutX="66.0" layoutY="130.0" prefHeight="301.0" prefWidth="684.0">
        <columns>
          <TableColumn fx:id="tripColumn" prefWidth="683.0" text="Tourguides going in all trips of that year" />
        </columns>
      </TableView>
      <Button fx:id="backBtn" layoutX="537.0" layoutY="463.0" mnemonicParsing="false" onAction="#onClickBack" prefHeight="35.0" prefWidth="211.0" text="Back" />
      <Label fx:id="errorLbl" alignment="CENTER" layoutX="76.0" layoutY="438.0" prefHeight="18.0" prefWidth="663.0" textFill="#8e1c1c" />
   </children>
</AnchorPane>
