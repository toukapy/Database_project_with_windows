      <Label layoutX="61.0" layoutY="62.0" text="Type the city:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="cityField" layoutX="189.0" layoutY="63.0" prefHeight="26.0" prefWidth="350.0" style="-fx-background-color: white;" />
      <TableView fx:id="tblEmployee" layoutX="63.0" layoutY="111.0" prefHeight="333.0" prefWidth="689.0" style="-fx-text-fill: green;">
        <columns>
          <TableColumn fx:id="employeeColumn" prefWidth="688.0" style="-fx-aligment: center;" text="Employee that frequents just one restaurant in the city" />
        </columns>
      </TableView>
      <Button fx:id="enterBtn" layoutX="576.0" layoutY="61.0" mnemonicParsing="false" onAction="#onClickEnter" prefHeight="30.0" prefWidth="114.0" text="Enter" />
      <Button fx:id="backBtn" layoutX="561.0" layoutY="511.0" mnemonicParsing="false" onAction="#onClickBack" prefHeight="30.0" prefWidth="182.0" text="Back" />
      <Label fx:id="errorLbl" alignment="CENTER" layoutX="64.0" layoutY="464.0" prefHeight="18.0" prefWidth="689.0" textFill="RED">
